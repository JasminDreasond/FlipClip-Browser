{
    "name": "__MSG_appName__",
    "short_name": "FlipClip",
    "description": "__MSG_appDesc__",
    "author": "Yasmin Seidel (JasminDreasond)",
    "default_locale": "en",
    "version": "1.0",
    "manifest_version": 3,
    "offline_enabled": false,
    "homepage_url": "https://puddy.club",
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "19": "icons/icon.png",
            "38": "icons/icon_38.png"
        }
    },
    "permissions": [
        "contentSettings",
        "webNavigation",
        "background",
        "storage",
        "unlimitedStorage",
        "activeTab",
        "webRequest",
        "history"
    ],
    "options_page": "index.html",
    "icons": {
        "128": "icons/icon_128.png",
        "16": "icons/icon_16.png",
        "48": "icons/icon_48.png"
    },
    "content_scripts": [{
        "js": ["startPage.js"],
        "matches": ["<all_urls>"],
        "run_at": "document_start"
    }],
    "host_permissions": ["<all_urls>"]
}